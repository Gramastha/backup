		function calcf(){
			var c= document.querySelector("#c").value;
			var f= c*9/5 +32;
			alert(c+" C is equal to "+f+" F")
		}
		
		function calc(){
			var f= document.querySelector("#f").value;
			var c= f*5/9 -32;
			alert(f+" F is equal to "+c+" C")
		}
	</script>
