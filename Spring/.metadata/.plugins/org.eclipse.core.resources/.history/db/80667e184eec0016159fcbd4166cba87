package com.psl.bean;

public class Employee {

	private String name;
	private int age;
	private Address address;
	
	public Employee() {
		System.out.println("IN emp >> default constructor");

	}
	
	public Employee(String name, int age) {
		super();
		System.out.println("IN emp >> para constructor");
		this.name = name;
		this.age = age;
		
	}

	public Employee(String name, int age, Address address) {
		super();
		System.out.println("IN emp >> para constructor");
		this.name = name;
		this.age = age;
		this.address = address;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		System.out.println("IN emp >> setName");
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		System.out.println("IN emp >> setAge");
		this.age = age;
	}

	public Address getAddress() {
		return address;
	}

	public void setAddress(Address address) {
		System.out.println("IN emp >> setAddress");
		this.address = address;
	}

	@Override
	public String toString() {
		return "Employee [name=" + name + ", age=" + age + ", address="
				+ address + "]";
	}
	
	public void abc() {
		System.out.println("IN ABC");

	}
	public void pqr() {
		System.out.println("IN pqr");

	}	
}
